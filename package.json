{
  "name": "youtube-sentiment-analyzer",
  "version": "1.0.0",
  "description": "A web application for analyzing YouTube video comment sentiment",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd . && python app.py",
    "frontend": "cd frontend && npm start",
    "install-deps": "pip install -r requirements.txt && cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "test": "cd frontend && npm test"
  },
  "keywords": [
    "youtube",
    "sentiment-analysis",
    "react",
    "flask",
    "nlp"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "modern-css-reset": "^1.4.0",
    "react-router-dom": "^7.6.3"
  }
}
